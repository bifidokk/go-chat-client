{
  "extends": ["tslint-angular"],
  "rules": {
    "directive-selector": [true, "attribute", "chat", "camelCase"],
    "component-selector": [true, "element", "chat", "kebab-case"],
    "import-spacing": true,
    "ordered-imports": [
      true,
      {
        "grouped-imports": true
      }
    ],
    "max-classes-per-file": false
  }
}
